CHAPTER 9 STRUCTURES  : IMPORTANT TOPIC




WRITE A PROGRAMME TO STORE THE DATA OF 1 STUDENTS.

# include <stdio.h>
# include <string.h>

 //user defiend 
 struct student {
    int roll;
    float cgpa;
    char name[100];
 };

 int main() {
    struct student s1;
    s1.roll = 1637;
    s1.cgpa = 9.7;
    //s1.name = "muqtadir";
    strcpy(s1.name,"muqtadir");

    printf("student name = %s\n", s1.name);
    printf("student roll no = %d\n", s1.roll);
    printf("student cgpa = %f\n" , s1.cgpa);

    return 0;
 }






WRITE A PROGRAMME TO STORE THE DATA OF 3 STUDENTS.


# include <stdio.h>
# include <string.h>

 //user defiend 
 struct student {
    int roll;
    float cgpa;
    char name[100];
 };

 int main() {
    struct student s1;
    s1.roll = 1637;
    s1.cgpa = 9.7;
    //s1.name = "muqtadir";
    strcpy(s1.name,"muqtadir");

    printf("student name = %s\n", s1.name);
    printf("student roll no = %d\n", s1.roll);
    printf("student cgpa = %f\n" , s1.cgpa);

struct student s2;
    s2.roll = 1638;
    s2.cgpa = 8.7;
    //s2.name = "juned";
    strcpy(s2.name,"juned");

    printf("student name = %s\n", s2.name);
    printf("student roll no = %d\n", s2.roll);
    printf("student cgpa = %f\n" , s2.cgpa);

struct student s3;
    s3.roll = 1639;
    s3.cgpa = 7.4;
    //s3.name = "hamza";
    strcpy(s3.name,"hamza");

    printf("student name = %s\n", s3.name);
    printf("student roll no = %d\n", s3.roll);
    printf("student cgpa = %f\n" , s3.cgpa);

    return 0;
 }







